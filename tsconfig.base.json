{
    "compilerOptions": {
        "target": "ES2022",
        "lib": [
            "ES2022"
        ],
        "moduleResolution": "node",
        "module": "CommonJS",
        "rootDir": "./",
        "baseUrl": "./",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noEmit": true,
        "noImplicitAny": true
    },
    "exclude": [
        "node_modules",
        "dist",
        ".serverless"
    ]
}